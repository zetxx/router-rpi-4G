{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "LCD",
            "program": "${workspaceFolder}/lcdService.js",
            "outputCapture": "std",
            "protocol": "inspector",
            "env": {
                "_statsapp_config__log__level": "trace",
                "statsapp_config__lcdAddr": 10,
                "statsapp_config__modem__repeatInterval": 5000,
                "statsapp_config__modemHealthCheck__repeatInterval": 5000
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "HTTP API",
            "program": "${workspaceFolder}/httpApi/service.js",
            "outputCapture": "std",
            "protocol": "inspector"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "HTTP CLIENT",
            "program": "${workspaceFolder}/httpClient/service.js",
            "outputCapture": "std",
            "protocol": "inspector"
        }
    ]
}